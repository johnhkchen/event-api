{
  "name": "agent-manager",
  "version": "1.0.0",
  "description": "Concurrent Claude Code Agent Management System",
  "main": "scripts/agent-manager.ts",
  "type": "module",
  "scripts": {
    "agent": "tsx scripts/agent-manager.ts",
    "agent:dev": "INIT_CWD=\"$PWD\" tsx scripts/agent-manager.ts",
    "agent:resume": "INIT_CWD=\"$PWD\" tsx scripts/agent-manager.ts resume",
    "agent:run": "INIT_CWD=\"$PWD\" tsx scripts/agent-manager.ts run",
    "build": "tsc",
    "dev": "tsx watch scripts/agent-manager.ts"
  },
  "bin": {
    "agent-manager": "./scripts/agent-manager.ts"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.21.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}